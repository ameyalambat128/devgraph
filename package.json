{
  "name": "devgraph",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "pnpm --filter web dev",
    "dev:studio": "pnpm --filter studio dev",
    "build": "turbo run build",
    "build:web": "pnpm --filter web build",
    "build:studio": "pnpm --filter studio build",
    "build:core": "pnpm --filter @devgraph/core build",
    "build:cli": "pnpm --filter devgraph build",
    "build:cli-full": "pnpm build:studio && pnpm --filter devgraph build:with-studio",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:core": "pnpm --filter @devgraph/core test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,json,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,json,css}\"",
    "devgraph": "pnpm exec tsx packages/devgraph-cli/src/index.ts",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build:core && pnpm build:cli-full && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "eslint": "9.12.0",
    "prettier": "3.3.3",
    "tsx": "4.19.1",
    "turbo": "latest"
  }
}
